<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="@dimen/_250mdp"
    android:layout_height="@dimen/_120mdp"
    android:background="@drawable/popup_back2">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/add_home_btn_3"
        android:layout_width="@dimen/_50mdp"
        android:layout_height="@dimen/_50mdp"
        android:layout_centerInParent="true"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/add_home_btn_2"
        app:layout_constraintEnd_toStartOf="@+id/add_home_btn_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/add_home_btn_2"
        app:layout_constraintVertical_bias="0.5"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/add_animation" />


    <TextView
        android:id="@+id/add_home_name_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5mdp"
        android:fontFamily="@font/prompt_regular"
        android:text="ADD"
        android:textColor="@color/white"
        android:textSize="@dimen/normal_font_size"
        app:layout_constraintEnd_toEndOf="@+id/add_home_btn_3"
        app:layout_constraintStart_toStartOf="@+id/add_home_btn_3"
        app:layout_constraintTop_toBottomOf="@+id/add_home_btn_3" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/add_home_btn_2"
        android:layout_width="@dimen/_50mdp"
        android:layout_height="@dimen/_50mdp"
        android:layout_centerInParent="true"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/add_home_btn_1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/add_home_btn_3"
        app:layout_constraintTop_toTopOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/add_animation" />

    <TextView
        android:id="@+id/add_home_name_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5mdp"
        android:fontFamily="@font/prompt_regular"
        android:text="Add"
        android:textColor="@color/white"
        android:textSize="@dimen/normal_font_size"
        app:layout_constraintEnd_toEndOf="@+id/add_home_btn_2"
        app:layout_constraintStart_toStartOf="@+id/add_home_btn_2"
        app:layout_constraintTop_toBottomOf="@+id/add_home_btn_2" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/add_home_btn_1"
        android:layout_width="@dimen/_50mdp"
        android:layout_height="@dimen/_50mdp"
        android:layout_centerInParent="true"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/add_home_btn_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/add_home_btn_2"
        app:layout_constraintTop_toTopOf="@+id/add_home_btn_2"
        app:layout_constraintVertical_bias="0.5"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/add_animation" />

    <TextView
        android:id="@+id/add_home_name_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5mdp"
        android:fontFamily="@font/prompt_regular"
        android:text="Home"
        android:textColor="@color/white"
        android:textSize="@dimen/normal_font_size"
        app:layout_constraintEnd_toEndOf="@+id/add_home_btn_1"
        app:layout_constraintStart_toStartOf="@+id/add_home_btn_1"
        app:layout_constraintTop_toBottomOf="@+id/add_home_btn_1" />
</androidx.constraintlayout.widget.ConstraintLayout>